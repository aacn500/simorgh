            "blocks": [
              {
                "type": "paragraph",
                "model": {
                  "text": "Northern Ireland regrouped from Friday's heavy opening defeat by Australia to earn a vital 67-50 win over Sri Lanka at the World Cup.",
                  "blocks": [
                    {
                      "type": "fragment",
                      "model": {
                        "text": "Northern Ireland regrouped from Friday's heavy opening defeat by Australia to earn a vital 67-50 win over Sri Lanka at the World Cup.",
                        "attributes": []
                      }
                    }
                  ]
                }
              },
              {
                "type": "paragraph",
                "model": {
                  "text": "The win boosts Northern Ireland's hopes of clinching the top-three spot in Group A which would see them progress to the next stage of the competition.",
                  "blocks": [
                    {
                      "type": "fragment",
                      "model": {
                        "text": "The win boosts Northern Ireland's hopes of clinching the top-three spot in Group A which would see them progress to the next stage of the competition.",
                        "attributes": []
                      }
                    }
                  ]
                }
              },
              {
                "type": "paragraph",
                "model": {
                  "text": "Emma Magee and half-time substitute Noleen Armstrong both scored heavily for the dominant Northern Irish.",
                  "blocks": [
                    {
                      "type": "fragment",
                      "model": {
                        "text": "Emma Magee and half-time substitute Noleen Armstrong both scored heavily for the dominant Northern Irish.",
                        "attributes": []
                      }
                    }
                  ]
                }
              },
              {
                "type": "paragraph",
                "model": {
                  "text": "Northern Ireland led 34-25 at half-time and remained in control.",
                  "blocks": [
                    {
                      "type": "fragment",
                      "model": {
                        "text": "Northern Ireland led 34-25 at half-time and remained in control.",
